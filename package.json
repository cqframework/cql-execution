  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "browserify": "^16.5.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.1.1",
    "should": "^13.2.3",
    "xml-js": "^1.6.11"
  },
  "dependencies": {
    "@lhncbc/ucum-lhc": "^4.1.3",
    "luxon": "^1.25.0"
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "lodash": "^4.17.19"
  },
  "main": "lib/cql",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prepublish": "npm run build --scripts-prepend-node-path && npm run build:browserify --scripts-prepend-node-path",
    "build": "babel src --out-dir lib",
    "build:browserify": "node ./bin/browserify.js",
    "build:test-data": "cd ./test/generator && ./gradlew generateTestData && cd ..",
    "build:spec-test-data": "node test/spec-tests/generate-cql.js && cd ./test/generator && ./gradlew generateSpecTestData && cd ..",
    "build:all": "npm run build --scripts-prepend-node-path && npm run build:browserify --scripts-prepend-node-path && npm run build:test-data --scripts-prepend-node-path && npm run build:spec-test-data --scripts-prepend-node-path",
    "watch:test-data": "cd ./test/generator && ./gradlew watchTestData && cd ..",
    "test": "mocha --reporter spec --recursive",
    "test:nyc": "nyc --reporter=html npx mocha --reporter spec --recursive",
    "test:watch": "mocha --reporter spec --recursive --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check \"**/*.js\"",
    "prettier:fix": "prettier --write \"**/*.js\"",
    "test:plus": "npm test --scripts-prepend-node-path && npm run build --scripts-prepend-node-path && npm run lint --scripts-prepend-node-path && npm run prettier --scripts-prepend-node-path"
  },
  "license": "Apache-2.0"
}
